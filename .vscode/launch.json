  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Server2",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server2.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "File",
      "program": "${file}",
    },
  ]
}